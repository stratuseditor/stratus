{ "name":        "stratus"
, "version":     "0.0.3"
, "description": "a web based programmer's text editor"
, "keywords":   ["stratus", "editor", "ide", "tool"]
, "homepage":    "http://stratuseditor.com/"
, "bugs":
  { "url":   "https://github.com/stratuseditor/stratus/issues"
  }
, "author":
  { "name": "sentientwaffle"
  , "url":  "http://sentientwaffle.github.com/"
  }

, "main": "./lib/fe/index"
, "bin": {"stratus": "./bin/stratus.js"}

, "scripts":
  { "test":       "make test"
