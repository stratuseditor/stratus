!!! 5
html
  head
    title= title
    
    // Styles
    link(rel='stylesheet', href='/images/iconic/font/iconic.css', type='text/css')
    link(rel="shortcut icon", href="/images/favicon.png")
    
    link(rel="stylesheet", href="/stylesheets/stratus-index.css")
    link(rel="stylesheet", href="/plugins.css")
    
    // The theme
    style.theme-css!= themeCSS(data.config["fractus.theme"])
    
    // Support files.
    script(type="text/javascript", src="/javascripts/support/underscore-min.js")
    script(type="text/javascript", src="/javascripts/support/jquery-1.7.min.js")
    script(type="text/javascript", src="/javascripts/support/jquery-ui-1.8.16.min.js")
    script(type="text/javascript", src="/socket.io/socket.io.js")
    
    // Some data
    script(type="text/javascript")!= jsData(data)
    // The browserify bundle.
    script(type="text/javascript", src="/stratus-browserify.js")
    script
      require.modules["stratus"]    = require.modules["/stratus/index.coffee"]
      require.modules["fractus"]    = require.modules["/fractus/index.js"]
      require.modules["stratus-ui"] = require.modules["/stratus-ui/index.js"]
    script(type="text/javascript", src="/plugins.js")
  body
    nav.toolbar: ul
      li File
      li Edit
      li View
      li Bundles
      li Tools
      li Help
    
    section.split
      .file-panel
        ul
      .main-panel
